{
    "name": "晋光的标签栏",
    "description": "一个有趣的标签栏!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "homepage_url": "https://code.idea4j.com/",
    "permissions": [
        "storage",
        "bookmarks",
        "contextMenus",
        "tabs",
        "notifications"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "just a tool",
        "default_icon": {              
           "16": "/images/icon16.png",
			"32": "/images/icon32.png",
			"48": "/images/icon48.png",
			"128": "/images/icon128.png"
    
        }
    },
	 "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "omnibox": {
        "keyword": "iphone"
    },
    "options_page": "options.html"
}